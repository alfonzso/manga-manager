{
  "port": "env(PORT)",
  "settings": {
    "loggerFormat": "tiny",
    "session": {
      "store": "@foal/typeorm",
      "expirationTimeouts": {
        "absolute": 1209600,
        "inactivity": 604800
      }
    }
  },
  "database": {
    "type": "postgres",
    "host": "env(DB_HOST)",
    "port": "env(DB_PORT)",
    "username": "env(DB_USERNAME)",
    "password": "env(DB_PASSWORD)",
    "database": "env(DB_NAME)"
  }
}
